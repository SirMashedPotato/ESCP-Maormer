<?xml version="1.0" encoding="utf-8" ?>
<Patch>


	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Vanilla Fishing Expanded</li>
		</mods>

		<match Class="PatchOperationSequence">
			<operations>


				<li Class="PatchOperationAdd"> 
					<xpath>/Defs</xpath>
					<value>

						<!-- other fishables -->

						<VCE_Fishing.FishDef>
							<defName>VCEF_ESCP_EggReefViperFertilized</defName>
							<thingDef>ESCP_EggReefViperFertilized</thingDef>    
							<fishSizeCategory>Special</fishSizeCategory>
							<canBeFreshwater>false</canBeFreshwater>
							<canBeSaltwater>true</canBeSaltwater>
							<allowedBiomes>		
								<li>Warm</li>	
								<li MayRequire="SirMashedPotato.MorrowRim">Ashlands</li>
							</allowedBiomes>  	
							<commonality>0.25</commonality>			
							<baseFishingYield>3</baseFishingYield>		
						</VCE_Fishing.FishDef>
						
					</value>
				</li>
			</operations>
		</match>
	</Operation>


</Patch>

