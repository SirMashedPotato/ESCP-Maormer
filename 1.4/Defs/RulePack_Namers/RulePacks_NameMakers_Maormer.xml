<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<RulePackDef>
		<defName>ESCP_NamerPersonMaormerMale</defName>
		<include>
		  <li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name(p=5)->[MaleList] [FamilyList]</li>
				<li>name(p=0.5)->[MaleList] '[Color] [serpents]' [FamilyList]</li>
				<li>name(p=0.1)->[MaleList] '[b] [serpents]' [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>MaleList</keyword>
					<path>ESCP_MaormerNames/MaormerMale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>ESCP_MaormerNames/MaormerFamily</path>
				</li>
			</rulesRaw>
			<rulesFiles>
				<li>b->ESCP_MaormerNames/MaormerFactionB</li>
				<li>serpents->ESCP_MaormerNames/MaormerNamesSerpents</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>ESCP_NamerPersonMaormerFemale</defName>
		<include>
		  <li>TribalWordUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name(p=5)->[FemaleList] [FamilyList]</li>
				<li>name(p=0.5)->[FemaleList] '[Color] [serpents]' [FamilyList]</li>
				<li>name(p=0.1)->[FemaleList] '[b] [serpents]' [FamilyList]</li>
			</rulesStrings>
			<rulesRaw>
				<li Class="Rule_File">
					<keyword>FemaleList</keyword>
					<path>ESCP_MaormerNames/MaormerFemale</path>
				</li>
				<li Class="Rule_File">
					<keyword>FamilyList</keyword>
					<path>ESCP_MaormerNames/MaormerFamily</path>
				</li>
			</rulesRaw>
			<rulesFiles>
				<li>b->ESCP_MaormerNames/MaormerFactionB</li>
				<li>serpents->ESCP_MaormerNames/MaormerNamesSerpents</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	
</Defs>